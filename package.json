{
  "name": "tokentoilet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install --prefer-offline --loglevel warn",
    "build": "next build",
    "dev": "next dev",
    "fix": "eslint --fix",
    "lint": "eslint",
    "prepare": "simple-git-hooks",
    "start": "next start",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,jsx,tsx,css,md,json,toml,yaml,yml}": [
      "eslint --fix"
    ],
    "package.json": [
      "pnpx sort-package-json"
    ]
  },
  "prettier": "@bfra.me/prettier-config/120-proof",
  "dependencies": {
    "@reown/appkit": "^1.7.18",
    "@reown/appkit-adapter-wagmi": "^1.7.18",
    "@tanstack/react-query": "^5.66.0",
    "lucide-react": "^0.542.0",
    "next": "15.5.3",
    "next-themes": "^0.4.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hot-toast": "^2.6.0",
    "wagmi": "^2.14.11"
  },
  "devDependencies": {
    "@bfra.me/eslint-config": "^0.30.2",
    "@bfra.me/prettier-config": "^0.16.0",
    "@eslint-react/eslint-plugin": "^1.53.1",
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^15.4.6",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "22.18.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "9.35.0",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "simple-git-hooks": "^2.13.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "4.1.13",
    "typescript": "5.9.2",
    "viem": "^2.23.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.1"
}

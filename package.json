{
  "name": "tokentoilet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install --prefer-offline --loglevel warn",
    "build": "next build",
    "dev": "next dev",
    "fix": "next lint --fix",
    "lint": "next lint",
    "prepare": "simple-git-hooks",
    "start": "next start"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,jsx,tsx,css,md,json,toml,yaml,yml}": [
      "eslint --fix"
    ],
    "package.json": [
      "pnpx sort-package-json"
    ]
  },
  "prettier": "@bfra.me/prettier-config/120-proof",
  "dependencies": {
    "@tanstack/react-query": "^5.66.0",
    "@web3modal/wagmi": "^5.1.11",
    "lucide-react": "^0.533.0",
    "next": "15.4.5",
    "next-themes": "^0.4.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "wagmi": "^2.14.11"
  },
  "devDependencies": {
    "@bfra.me/eslint-config": "^0.25.1",
    "@bfra.me/prettier-config": "^0.16.0",
    "@next/eslint-plugin-next": "^15.4.6",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "22.17.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "autoprefixer": "10.4.21",
    "eslint": "9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "lint-staged": "^16.1.5",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "4.1.11",
    "typescript": "5.8.3",
    "viem": "^2.23.0"
  },
  "packageManager": "pnpm@10.14.0"
}
